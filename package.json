{
  "name": "music-vue-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "cross-env THEME=red IS_WEB=true vue-cli-service serve",
    "serve:blue": "cross-env THEME=blue IS_WEB=true vue-cli-service serve",
    "build": "cross-env THEMES=blue,red,pink,green,golden IS_WEB=true vue-cli-service build",
    "lint": "vue-cli-service lint",
    "fix": "eslint --fix --ext .js,.ts,.vue src",
    "proxy": "set IS_WEB=true& node ./src/proxy",
    "electron:build": "cross-env THEMES=blue,red,pink,green,golden vue-cli-service electron:build",
    "electron:serve": "cross-env vue-cli-service electron:serve",
    "electron:serve:blue": "cross-env THEME=blue vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,html,vue,ts}": [
      "prettier --config .prettierrc.js --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "apicache": "^1.5.3",
    "axios": "^0.19.1",
    "babel-polyfill": "^6.26.0",
    "big-integer": "^1.6.48",
    "core-js": "^2.6.5",
    "crypto": "^1.0.1",
    "element-ui": "^2.13.0",
    "js-cookie": "^2.2.1",
    "pac-proxy-agent": "^4.1.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vue-video-player": "^5.0.2",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.0",
    "@vue/cli-plugin-eslint": "^3.12.0",
    "@vue/cli-plugin-typescript": "^3.12.0",
    "@vue/cli-service": "^3.12.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.13.0",
    "cross-env": "^6.0.3",
    "electron": "^18.3.7",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^4.0.10",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "typescript": "^3.4.3",
    "vue-cli-plugin-electron-builder": "^1.4.4",
    "vue-template-compiler": "^2.6.10"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "__npminstall_done": false
}
